<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.server.mapper.ServerlistHisMapper">


<select id="type" resultType="com.test.server.entity.ServerlistHis">
    SELECT
     *
    FROM
      serverlisthis

    <where>
      <if test="type != null and type != ''">
        serverlist.type LIKE concat('%',#{type},'%')
      </if>

    </where>
    ORDER BY id DESC
</select>




</mapper>
